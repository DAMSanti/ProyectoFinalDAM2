[Unit]
Description=ACEX API - Activity Management System
After=network.target

[Service]
WorkingDirectory=/var/www/acexapi/publish
ExecStart=/snap/bin/dotnet /var/www/acexapi/publish/ACEXAPI.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=acexapi
User=root
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false

[Install]
WantedBy=multi-user.target
